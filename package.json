{
    "name": "uber-reports-etl",
    "version": "1.0.0",
    "type": "commonjs",
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "start": "node dist/job.js",
        "chmod": "chmod +x dist/*",
        "job": "npm run build > /dev/null && npm run chmod > /dev/null && node dist/bin/testSettlementBreakdown.js",
        "lint": "eslint \"src/**/*.ts\"",
        "lint:fix": "eslint \"src/**/*.ts\" --fix",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "devDependencies": {
        "@types/csv-parse": "^1.1.12",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^8.53.0",
        "@typescript-eslint/parser": "^8.53.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "jest": "^30.2.0",
        "prettier": "^3.7.4",
        "ts-jest": "^29.4.6",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "csv-parse": "^6.1.0",
        "csv-stringify": "^6.6.0",
        "decimal.js": "^10.6.0"
    }
}
